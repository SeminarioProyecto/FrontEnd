    "@ngx-formly/material": "^5.5.5",
    "@ngx-progressbar/core": "~5.3.2",
    "@ngx-progressbar/router": "~5.3.2",
    "@ngx-translate/core": "~11.0.1",
    "@ngx-translate/http-loader": "~4.0.0",
    "apexcharts": "^3.10.1",
    "axios": "^0.19.0",
    "core-js": "^3.1.4",
    "hammerjs": "^2.0.8",
    "ngx-popper": "^6.2.0",
    "ngx-toastr": "^11.2.1",
    "photoviewer": "^3.4.0",
    "popper.js": "^1.15.0",
